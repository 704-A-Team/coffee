<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.oracle.coffee.ClientMapper">

	<select id="clientInfo" parameterType="int" resultType="SWClientDto">
		select DISTINCT client_code, client_name
		from client_tb
		where client_type = #{client_type}
	</select>


</mapper>